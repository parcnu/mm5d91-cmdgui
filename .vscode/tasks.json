{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build with G++ 14.2.0",
			"command": "/usr/local/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++20",
				"-g",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/usergui"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/local/bin/g++"
		},
		{
			"type": "cppbuild",
			"label": "Build with G++ 10.2.1",
			"command": "/usr/bin/aarch64-linux-gnu-g++",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++20",
				"-g",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/usergui"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/aarch64-linux-gnu-g++"
		}
	]
}